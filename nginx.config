events {}
http {
    access_log /var/log/nginx/access.log;
    error_log /var/log/nginx/error.log;

    server {
        listen 0.0.0.0:5577;
        server_name localhost;

        location / {
            proxy_pass http://ollama:11343;
        }

        location /protected {
            auth_basic "Restricted Content";
            auth_basic_user_file /etc/nginx/.htpasswd;
        }
    }
}